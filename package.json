{
  "name": "@zent/theme-sdk",
  "version": "0.0.1",
  "description": "The theme SDK for Zent",
  "main": "./cmd/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cmd",
    "build:cmd": "rimraf cmd && tsc --module commonjs --esModuleInterop --outDir cmd",
    "build:esm": "rimraf esm && tsc --module es2015 --outDir esm"
  },
  "keywords": [
    "zent",
    "theme",
    "sdk"
  ],
  "files": [
    "cmd/",
    "esm/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zent-contrib/theme-sdk.git"
  },
  "author": "Youzan FE",
  "license": "MIT",
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^15.12.5",
    "rimraf": "^3.0.2"
  }
}
